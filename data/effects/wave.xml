<!DOCTYPE kpartgui>
<effect tag="wave" id="wave">
	<name context="Wave Effect Name">Wave</name>
	<description>Make waves on your clip with keyframes</description>
	<author>Leny Grisel</author>
	<parameter type="animated" name="start" max="100" min="0" default="10">
		<name>Amplitude</name>
	</parameter>
    <parameter type="animated" name="end" max="100" min="1" default="10">
		<name>End Amplitude</name>
	</parameter>
    <parameter type="animated" name="wave" max="100" min="1" default="10">
		<name>Wave Amplitude</name>
	</parameter>
    <parameter type="animated" name="speed" max="100" min="0" default="5">
		<name>Speed</name>
	</parameter>
	<parameter type="bool" name="deformX" default="1">
		<name>Horizontal</name>
	</parameter>
	<parameter type="bool" name="deformY" default="0">
		<name>Vertical</name>
	</parameter>
</effect>
