<!DOCTYPE kpartgui>
<effect tag="frei0r.bigsh0t_transform_360" id="frei0r.bigsh0t_transform_360">
	<name>VR360 Transform</name>
	<description>Rotates a panoramic image.</description>
	<author>Leo Sutic</author>
	
    <parameter type="animated" name="yaw" default="0" min="-360" max="360" factor="1" suffix="deg">
		<name>yaw</name>
	</parameter>
    <parameter type="animated" name="pitch" default="0" min="-180" max="180" factor="1" suffix="deg">
		<name>pitch</name>
	</parameter>
    <parameter type="animated" name="roll" default="0" min="-180" max="180" factor="1" suffix="deg">
		<name>roll</name>
	</parameter>
	<parameter type="list" name="interpolation" default="0" paramlist="0;1">
                <paramlistdisplay>Nearest-Neighbor,Bilinear</paramlistdisplay>
                <name>Interpolation</name>
        </parameter>
</effect>
